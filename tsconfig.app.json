{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "exclude": ["node_modules", "public"],
  "include": ["src/**/*", "*.d.ts"],
  "compilerOptions": {
    "allowJs": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
